{
  "name": "webda",
  "version": "0.5.2",
  "description": "Expose API with Lambda",
  "keywords": [
    "aws",
    "lamda",
    "api gateway"
  ],
  "author": "Remi Cattiau <remi@cattiau.com>",
  "repository": "git://github.com/loopingz/webda.git",
  "main": "immutable",
  "scripts": {
    "test": "mocha -t 20000",
    "run": "node core",
    "format": "node ./scripts/format.js",
    "cover": "node_modules/.bin/istanbul cover --report lcovonly --dir ./reports _mocha -- -u exports -R spec",
    "coverhtml": "node_modules/.bin/istanbul cover --report html --dir ./reports _mocha -- -u exports -R spec"
  },
  "dependencies": {
    "accept-language": "=3.0.15",
    "ajv": ">=5.2.3",
    "aws-sdk": ">=2.24.0",
    "cookie": ">=0.2.3",
    "email-templates": "^2.7.1",
    "fs-finder": "^1.8.1",
    "mime": ">=2.0.3",
    "mime-types": "^2.1.11",
    "mongodb": ">=2.1.7",
    "mustache": "^2.2.1",
    "nodemailer": "^4.0.1",
    "nodemailer-ses-transport": "^1.3.0",
    "passport": "=0.2.2",
    "passport-amazon": "^1.0.0",
    "passport-facebook": ">=2.0.0",
    "passport-github2": ">=0.1.9",
    "passport-google-oauth": "=0.2.0",
    "passport-twitter": ">=1.0.3",
    "uri-templates": ">=0.2.0",
    "uuid": "^3.0.0"
  },
  "files": [
    "core.js",
    "handlers",
    "models",
    "policies",
    "queues",
    "routehelpers",
    "services",
    "stores",
    "utils"
  ],
  "devDependencies": {
    "glob": "^7.1.2",
    "istanbul": "^0.4.5",
    "js-beautify": "^1.7.5",
    "mocha": ">=2.2.5"
  },
  "homepage": "https://github.com/loopingz/webda"
}
